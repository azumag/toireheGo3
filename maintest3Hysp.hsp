*ball
	POLYS = 20 ; 球体分割数
	dh = 6.2831853 / POLYS


	r = 200


	; oOOo
	repeat POLYS / 2 - 1, 1

		ph = dh * cnt

		d3initlineto

		repeat POLYS + 1
			th = dh * cnt

			xb = 00
			yb = 00
			zb = r

			; 座標を回転置換
			d3rotate yb, zb, yb, zb, ph
			d3rotate xb, yb, xb, yb, th
			
			d3lineto xb, yb, zb
		loop
	loop


	; (())
	ph = 0.0

	repeat POLYS

		th = dh * cnt

		d3initlineto

		repeat POLYS / 2 - 1, 1
			ph = dh * cnt

			xb = 00
			yb = 00
			zb = r

			; 座標を回転置換
			d3rotate yb, zb, yb, zb, ph
			d3rotate xb, yb, xb, yb, th

			d3lineto xb, yb, zb
